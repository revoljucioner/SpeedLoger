<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Log"
             x:Class="Log.Pages.MainPage">
    
    <StackLayout x:Name="stackLayout">
        <StackLayout.Children>
            <Button x:Name="buttonOpenRecordPage" Text="Open record page" Clicked="ButtonOpenRecordPage_Clicked" />
            <Button x:Name="buttonOpenRecordsListPage" Text="Open records list page" Clicked="ButtonOpenRecordsListPage_Clicked" />
        </StackLayout.Children>
    </StackLayout> 
    
</ContentPage>
